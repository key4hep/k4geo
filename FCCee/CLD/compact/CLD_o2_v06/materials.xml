    <material formula="Al" name="Aluminium" state="solid" >
      <D type="density" unit="g/cm3" value="2.699" />
      <composite n="1" ref="Al" />
    </material>

    <material name="PyrexGlass">
      <D type="density" value="2.23" unit="g/cm3"/>
